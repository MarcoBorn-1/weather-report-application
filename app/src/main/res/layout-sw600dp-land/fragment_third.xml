<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:columnCount="3"
    android:rowCount="6"
    android:background="#DDF6C4"
    tools:context=".ThirdFragment">


    <ImageView
        android:id="@+id/weather1Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/weather3Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toEndOf="@+id/weather2Image"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/weather4Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather1Temp" />

    <ImageView
        android:id="@+id/weather6Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toEndOf="@+id/weather5Image"
        app:layout_constraintTop_toBottomOf="@+id/weather3Temp" />

    <ImageView
        android:id="@+id/weather5Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="16dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toEndOf="@+id/weather4Image"
        app:layout_constraintTop_toBottomOf="@+id/weather2Temp" />

    <TextView
        android:id="@+id/weather1Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather1Image" />

    <TextView
        android:id="@+id/weather2Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="1"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather1Time"
        app:layout_constraintTop_toBottomOf="@+id/weather2Image" />

    <TextView
        android:id="@+id/weather6Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="12dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather5Temp"
        app:layout_constraintTop_toBottomOf="@+id/weather6Time" />

    <TextView
        android:id="@+id/weather1Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_row="0"
        android:layout_column="0"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather1Time" />

    <TextView
        android:id="@+id/weather2Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather1Temp"
        app:layout_constraintTop_toBottomOf="@+id/weather2Time" />

    <TextView
        android:id="@+id/weather3Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather2Time"
        app:layout_constraintTop_toBottomOf="@+id/weather3Image" />

    <TextView
        android:id="@+id/weather3Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather2Temp"
        app:layout_constraintTop_toBottomOf="@+id/weather3Time" />

    <TextView
        android:id="@+id/weather4Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather4Image" />

    <TextView
        android:id="@+id/weather4Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/weather4Time" />

    <TextView
        android:id="@+id/weather5Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather4Temp"
        app:layout_constraintTop_toBottomOf="@+id/weather5Image" />

    <TextView
        android:id="@+id/weather5Temp"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather4Time"
        app:layout_constraintTop_toBottomOf="@+id/weather5Time" />

    <TextView
        android:id="@+id/weather6Time"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="140dp"
        android:layout_marginTop="16dp"
        android:text="@string/none"
        android:textSize="24sp"
        app:layout_constraintStart_toEndOf="@+id/weather5Time"
        app:layout_constraintTop_toBottomOf="@+id/weather6Image" />

    <ImageView
        android:id="@+id/weather2Image"
        android:layout_width="200dp"
        android:layout_height="150dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="8dp"
        android:src="@mipmap/ic_launcher"
        app:layout_constraintStart_toEndOf="@+id/weather1Image"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>